{
  "name" : "codeages/escloud-php-sdk",
  "type" : "library",
  "description" : "ESCloud PHP SDK",
  "authors": [
    {
      "name": "Codeages Team",
      "email": "team@codeages.com",
      "homepage": "http://www.codeages.com"
    }
  ],
  "autoload" : {
    "files": ["src/functions.php"],
    "psr-4": {
      "ESCloud\\SDK\\" : "src"
    }
  },
  "autoload-dev" : {
    "psr-4": {
      "ESCloud\\SDK\\Tests\\" : "tests"
    }
  },
  "require": {
    "php": ">=5.3.3",
    "ext-json": "*",
    "ext-curl": "*",
    "psr/log": "^1.0",
    "firebase/php-jwt": "^5.0",
    "paragonie/random_compat": ">=2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4 || ^5 || ^6",
    "monolog/monolog": "^1.18"
  }
}
